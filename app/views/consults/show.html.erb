<h1>
    <%= @consult.coverage %>
</h1>

<h3>
  By <%= @consult.medic.email %>
</h3>
<p>
    <strong>Description:</strong>
    <%= @consult.description %>
</p>

<% if policy(@consult).edit?%>
  <%= link_to 'Edit', edit_consult_path(@consult)%>
<%end%>

<% if policy(@consult).destroy?%>
  <%= link_to 'Delete', consult_path(@consult), method: :delete %>
<%end%>

<%= link_to 'Back to Index', consults_path %>
